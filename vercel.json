{
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index.py" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors *" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
